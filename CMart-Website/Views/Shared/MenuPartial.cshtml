@model IEnumerable<CMart_Website.Models.ProductType>

@{
    ViewBag.Title = "partial";
}

<div class="span3 col">
    <div class="block">
        <ul class="nav nav-list">
            <li class="nav-header">CATEGORIES</li>
            @foreach (var item in Model)
            {
                <li>@Html.ActionLink(@item.Name, "Category", "Home", new { IDType = @item.IDType }, null)</li>
            }
        </ul>

    </div>
</div>
